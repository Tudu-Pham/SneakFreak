{
  "name": "sneakfreak",
  "version": "1.0.0",
  "description": "sneaker shopping online web",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon",
    "dev": "nodemon",
    "start:debug": "nodemon --inspect=9229 -e ts,tsx --exec node -r ts-node/register ./src/server.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "node_modules"
    ],
    "exec": "ts-node ./src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tudu-Pham/SneakFreak.git"
  },
  "keywords": [
    "nodejs"
  ],
  "author": "Pham Trung Dung, Nguyen Le Phuong Anh, Nguyen Duc Duy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tudu-Pham/SneakFreak/issues"
  },
  "homepage": "https://github.com/Tudu-Pham/SneakFreak#readme",
  "dependencies": {
    "dotenv": "16.4.7",
    "ejs": "3.1.10",
    "express": "5.0.1"
  },
  "devDependencies": {
    "@types/ejs": "3.1.5",
    "@types/express": "5.0.0",
    "@types/node": "22.10.7",
    "nodemon": "3.1.9",
    "ts-node": "10.9.2",
    "typescript": "5.7.3"
  }
}